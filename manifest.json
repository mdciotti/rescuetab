{
	"manifest_version": 2,
	"name": "RescueTab",
	"permissions": [ "sessions", "tabs", "unlimitedStorage", "chrome://favicon/", "http://*/*", "https://*/*" ],
	"version": "0.0.1",
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_icon": {
			"19": "icon/icon2.png",
			"38": "icon/icon2.png"
		},
		"default_popup": "index.html",
		"default_title": "RescueTab"
	},
	"description": "Rescue your accidentally closed tabs from the murky depths of tab limbo!",
	"icons": {
		"128": "icon/icon2.png",
		"16": "icon/icon2.png",
		"48": "icon/icon2.png"
	}
}
